{
  "displayName": "Table",
  "defaultWidth": 600,
  "defaultHeight": 400,
  "resizeX": true,
  "resizeY": true,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "columns",
      "displayName": "Columns",
      "type": "list"
    },
    {
      "name": "rows",
      "displayName": "Rows",
      "type": "list"
    },
    {
      "name": "height",
      "displayName": "Height",
      "type": "text",
      "default": "auto",
      "control": {
        "type": "menu",
        "options": [
          {
            "value": "auto",
            "label": "Auto"
          },
          {
            "value": "manual",
            "label": "Component Height"
          }
        ]
      }
    },
    {
      "name": "density",
      "displayName": "Density",
      "type": "text",
      "default": "standard",
      "control": {
        "type": "menu",
        "options": [
          {
            "value": "compact",
            "label": "Compact"
          },
          {
            "value": "standard",
            "label": "Standard"
          },
          {
            "value": "confortable",
            "label": "Confortable"
          }
        ]
      }
    }
  ],
  "childComponents": [
    {
      "name": "Column Header",
      "role": "listItem",
      "reference": "columns",
      "props": [
        {
          "name": "field",
          "displayName": "Field",
          "type": "text"
        },
        {
          "name": "headerName",
          "displayName": "Header Name Field",
          "type": "text"
        },
        {
          "name": "width",
          "displayName": "Width",
          "type": "number",
          "default": 200
        }
      ]
    }
  ]
}
