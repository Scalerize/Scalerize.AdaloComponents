{
  "displayName": "Treemap",
  "defaultWidth": 300,
  "defaultHeight": 200,
  "resizeX": true,
  "resizeY": true,
  "components": "./index.js",
  "icon": "./thumbnail.png",
  "props": [
    {
      "name": "items",
      "displayName": "Treemap Items",
      "type": "list"
    }
  ],
  "childComponents": [
    {
      "name": "item",
      "displayName": "Item",
      "role": "listItem",
      "reference": "items",
      "props": [
        {
          "name": "title",
          "displayName": "Title",
          "type": "text",
          "default": "Sample"
        },
        {
          "name": "value",
          "displayName": "Value",
          "type": "number",
          "default": 10
        }
      ]
    }
  ]
}
