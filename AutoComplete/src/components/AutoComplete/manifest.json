{
  "displayName": "Suggestion Box",
  "defaultWidth": 160,
  "defaultHeight": 40,
  "resizeX": true,
  "resizeY": false,
  "components": "./index.js",
  "icon": "./thumbnail.png",
  "props": [
  ],
  "childComponents": [
    {
      "name": "searchField",
      "displayName": "Search Field",
      "props": [
        {
          "name": "search",
          "displayName": "Search",
          "type": "text",
          "role": "formValue"
        },
        {
          "name": "placeholder",
          "displayName": "Placeholder",
          "type": "text",
          "default": "Search"
        },
        {
          "name": "border",
          "displayName": "Border",
          "type": "object",
          "control": {
            "type": "border"
          },
          "default": {
            "borderColor": "#bbbbbb",
            "borderWidth": 1,
            "borderStyle": "solid"
          },
          "style": true
        },
        {
          "name": "borderRadius",
          "displayName": "Border Radius",
          "type": "number",
          "control": {
            "type": "slider",
            "min": 0,
            "max": 18
          },
          "default": 4,
          "style": true
        },
        {
          "name": "backgroundColor",
          "displayName": "Background Color",
          "type": "color",
          "default": "#ffffff",
          "style": true
        },
        {
          "name": "leftIcon",
          "displayName": "Left Icon",
          "type": "icon",
          "default": "search",
          "style": true
        },
        {
          "name": "textColor",
          "displayName": "Text Color",
          "type": "color",
          "default": "#000000",
          "style": true
        },
        {
          "name": "placeholderColor",
          "displayName": "Placeholder Color",
          "type": "color",
          "default": "#e0e0e0",
          "style": true
        },
        {
          "name": "shadow",
          "displayName": "Shadow",
          "type": "object",
          "control": {
            "type": "shadow"
          },
          "default": {
            "enabled": true,
            "x": 0,
            "y": 2,
            "size": 4,
            "color": "rgba(0, 0, 0, 0.2)"
          },
          "style": true
        }
      ]
    },
    {
      "name": "suggestionsOverlay",
      "displayName": "Suggestions Overlay",
      "props": [
        {
          "name": "overlayType",
          "displayName": "Overlay type",
          "type": "text",
          "default": "wrap",
          "control": {
            "type": "menu",
            "options": [
              {
                "label": "Wrap",
                "value": "wrap"
              },
              {
                "label": "Separated",
                "value": "separated"
              },
              {
                "label": "Attached",
                "value": "attached"
              }
            ]
          },
          "style": true
        },
        {
          "name": "shadow",
          "displayName": "Shadow",
          "type": "object",
          "control": {
            "type": "shadow"
          },
          "default": {
            "enabled": true,
            "x": 0,
            "y": 2,
            "size": 4,
            "color": "rgba(0, 0, 0, 0.2)"
          },
          "style": true
        },
        {
          "name": "border",
          "displayName": "Border",
          "type": "object",
          "control": {
            "type": "border"
          },
          "default": {
            "borderColor": "#ffffff00",
            "borderWidth": 1,
            "borderStyle": "solid"
          },
          "style": true
        },
        {
          "name": "borderRadius",
          "displayName": "Border radius",
          "type": "number",
          "control": {
            "type": "slider",
            "min": 0,
            "max": 18
          },
          "default": 4,
          "style": true
        },
        {
          "name": "suggestionsList",
          "displayName": "Suggestions list",
          "type": "list",
          "style": false
        }
      ]
    },
    {
      "name": "suggestion",
      "displayName": "Suggestion",
      "role": "listItem",
      "reference": "suggestionsList",
      "props": [
        {
          "name": "onSuggestionClick",
          "type": "action",
          "displayName": "On Suggestion Click",
          "arguments": []
        },
        {
          "name": "leftType",
          "displayName": "Left content type",
          "type": "text",
          "default": "icon",
          "control": {
            "type": "menu",
            "options": [
              {
                "label": "Icon",
                "value": "icon"
              },
              {
                "label": "Image",
                "value": "image"
              },
              {
                "label": "None",
                "value": "none"
              }
            ]
          }
        },
        {
          "name": "leftIcon",
          "displayName": "Left icon",
          "type": "icon",
          "enabled": {
            "leftType": "icon"
          },
          "default": "star"
        },
        {
          "name": "leftBackgroundColor",
          "displayName": "Icon background",
          "type": "color",
          "enabled": {
            "leftType": "icon"
          },
          "default": "@secondary"
        },
        {
          "name": "leftForegroundColor",
          "displayName": "Icon foreground",
          "type": "color",
          "enabled": {
            "leftType": "icon"
          },
          "default": "@primary"
        },
        {
          "name": "leftImage",
          "displayName": "Left image",
          "type": "image",
          "enabled": {
            "leftType": "image"
          }
        },
        {
          "name": "titleContent",
          "displayName": "Title",
          "type": "text",
          "default": "Title"
        },
        {
          "name": "titleColor",
          "displayName": "Title color",
          "type": "color",
          "default": "@primary"
        },
        {
          "name": "subtitleContent",
          "displayName": "Subtitle",
          "type": "text",
          "default": "Subtitle"
        },
        {
          "name": "subtitleColor",
          "displayName": "Subtitle color",
          "type": "color",
          "default": "@primary"
        },
        {
          "name": "buttonType",
          "displayName": "Right button type",
          "type": "text",
          "default": "icon",
          "control": {
            "type": "menu",
            "options": [
              {
                "label": "Text Button",
                "value": "text"
              },
              {
                "label": "Icon Button",
                "value": "icon"
              },
              {
                "label": "None",
                "value": "none"
              }
            ]
          }
        },
        {
          "name": "buttonText",
          "displayName": "Right button text",
          "type": "text",
          "enabled": {
            "buttonType": "text"
          },
          "default": "Click Here"
        },
        {
          "name": "buttonIcon",
          "displayName": "Right button icon",
          "type": "icon",
          "enabled": {
            "buttonType": "icon"
          },
          "default": "arrow_forward"
        },
        {
          "name": "buttonForegroundColor",
          "displayName": "Right button foreground",
          "type": "color",
          "default": "@primary"
        },
        {
          "name": "buttonBckgroundColor",
          "displayName": "Right button background",
          "type": "color",
          "default": "@secondary"
        },
        {
          "name": "buttonOnClick",
          "type": "action",
          "displayName": "On Button Click",
          "arguments": []
        }
      ]
    }
  ]
}
